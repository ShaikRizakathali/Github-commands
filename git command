ssh-keygen -t rsa -b 4096 -C "shaikhrizakathali@gmail.com"

eval "$(ssh-agent -s)"

ssh-add ~/.ssh/id_rsa

cat ~/.ssh/id_rsa.pub
# Copy the output and add it to your Git hosting service (e.g., GitHub, GitLab, Bitbucket) under SSH keys in your account settings. 
